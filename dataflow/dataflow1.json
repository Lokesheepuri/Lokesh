{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "src_data_scd",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "snk_data_scd",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "snk_data_scd2",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"dataset": {
						"referenceName": "snk_data_scd3",
						"type": "DatasetReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "exists1"
				},
				{
					"name": "exists2"
				},
				{
					"name": "alterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          student_id as integer,",
				"          student_name as string,",
				"          department as string,",
				"          Marks as integer,",
				"          city as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          student_id as integer,",
				"          student_name as string,",
				"          department as string,",
				"          Marks as integer,",
				"          city as string,",
				"          Hash_key as string,",
				"          status as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2",
				"source1 derive(hash_key1 = md5(student_name,department,Marks,city),",
				"          status = 1) ~> derivedColumn1",
				"source2 derive(hash_key2 = md5(student_name,department,Marks,city),",
				"          {status()} = 0) ~> derivedColumn2",
				"derivedColumn1, derivedColumn2 exists(source1@student_id == source2@student_id && hash_key1 == hash_key2,",
				"     negate:true,",
				"     broadcast: 'auto')~> exists1",
				"derivedColumn2, derivedColumn1 exists(source2@student_id == source1@student_id && hash_key2 != hash_key1,",
				"     negate:false,",
				"     broadcast: 'auto')~> exists2",
				"exists2 alterRow(updateIf(1==1)) ~> alterRow1",
				"exists1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          student_id as integer,",
				"          student_name as string,",
				"          department as string,",
				"          Marks as integer,",
				"          city as string,",
				"          Hash_key as string,",
				"          status as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          student_id,",
				"          student_name,",
				"          department,",
				"          Marks,",
				"          city,",
				"          Hash_key = hash_key1,",
				"          status",
				"     )) ~> sink1",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          student_id as integer,",
				"          student_name as string,",
				"          department as string,",
				"          Marks as integer,",
				"          city as string,",
				"          Hash_key as string,",
				"          status as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:true,",
				"     upsertable:false,",
				"     keys:['student_id'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          student_id,",
				"          student_name,",
				"          department,",
				"          Marks,",
				"          city,",
				"          Hash_key = hash_key2,",
				"          status = {status()}",
				"     )) ~> sink2"
			]
		}
	}
}